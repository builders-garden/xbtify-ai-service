{
  "name": "ai-service-be",
  "version": "1.0.0",
  "description": "Express API service with BullMQ scheduled jobs",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
