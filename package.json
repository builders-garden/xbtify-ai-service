    "bullmq": "^5.41.8",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.44.6",
    "express": "^4.21.2",
    "express-basic-auth": "^1.2.1",
    "helmet": "^7.1.0",
    "ioredis": "^5.8.1",
    "ky": "^1.12.0",
    "pg": "^8.16.3",
    "redis": "^4.7.0",
    "redlock": "^5.0.0-beta.2",
