{
	"name": "ai-service-be",
	"version": "1.0.0",
	"description": "Express API service with BullMQ scheduled jobs",
	"main": "dist/index.js",
	"type": "module",
	"scripts": {
		"db:generate": "drizzle-kit generate",
		"db:migrate": "drizzle-kit migrate",
		"db:pull": "drizzle-kit pull",
		"db:push": "drizzle-kit push",
		"db:studio": "drizzle-kit studio",
		"check": "biome check --write .",
		"format": "npx @biomejs/biome format --write ./src",
		"start": "node dist/index.js",
		"dev": "nodemon --ignore dist/ --exec 'npm run build && npm start' --ext ts,js,json",
		"build": "tsc",
		"build:watch": "tsc --watch",
		"lint": "eslint . --ext .ts",
		"test": "jest"
	},
	"dependencies": {
		"@bull-board/api": "^6.9.6",
		"@bull-board/express": "^6.9.6",
		"@farcaster/core": "^0.18.9",
		"@farcaster/frame-sdk": "^0.0.31",
		"@farcaster/hub-nodejs": "^0.15.9",
		"@farcaster/miniapp-core": "^0.4.1",
		"@langchain/openai": "^0.3.14",
		"@langchain/core": "^0.3.21",
		"@langchain/langgraph": "^0.2.20",
		"langfuse-langchain": "^3.28.0",
		"@neynar/nodejs-sdk": "^3.34.0",
		"bullmq": "^5.41.8",
		"cors": "^2.8.5",
		"dotenv": "^16.3.1",
		"drizzle-orm": "^0.44.6",
		"express": "^4.21.2",
		"express-basic-auth": "^1.2.1",
		"helmet": "^7.1.0",
		"ioredis": "^5.8.1",
		"ky": "^1.12.0",
		"pg": "^8.16.3",
		"ulid": "^3.0.1",
		"uuid": "^11.1.0",
		"viem": "^2.31.6",
		"zod": "^3.24.2"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.2.6",
		"@types/cors": "^2.8.17",
		"@types/express": "^4.17.21",
		"@types/jest": "^29.5.11",
		"@types/node": "^20.10.5",
		"@types/pg": "^8.15.5",
		"drizzle-kit": "^0.31.4",
		"jest": "^29.7.0",
		"nodemon": "^3.0.1",
		"ts-jest": "^29.1.1",
		"ts-node-dev": "^2.0.0",
		"typescript": "^5.3.3",
		"ultracite": "^5.6.4"
	}
}
